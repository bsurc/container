#!/bin/bash
#SBATCH -J opensmile
#SBATCH --mail-type=ALL
#SBATCH --mail-user=kyleshannon@boisestate.edu
#SBATCH -o logs/opensmile-%J-%A-%a.log
#SBATCH --cpus-per-task 24
